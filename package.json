{
    "name": "bounty-hunter",
    "version": "1.0.0",
    "description": "A job application tracking app",
    "main": "server/server.js",
    "scripts": {
        "start": "node server/server.js",
        "server": "nodemon server/server.js",
        "client": "live-server client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install-client": "cd client && npm install",
        "install-server": "cd server && npm install",
        "install-all": "npm run install-client && npm run install-server"
    },
    "author": "Ezekiel Owens",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.12.3",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "concurrently": "^6.2.1",
        "live-server": "^1.2.1",
        "nodemon": "^2.0.7"
    }
}
